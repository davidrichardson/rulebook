{
  "name": "spacecraft checklist",
  "description": "a geeky test checklist",
  "ruleGroups": [
    {
      "name": "common core rules",
      "restrictionType": "any",
      "rules": [
        {
          "name": "role",
          "locatorPath": {"path": "$.attributes[?($.name == 'role')]"},
          "requirementStrength": "mandatory",
          "multiplicty": "many",
          "validators": [
            {
              "locator": {"path": "$.value"},
              "type": "FreeText", "config": {"minNumberOfCharacters": 3, "maxNumberOfCharacters": 50}
            }
          ]
        }

      ]
    }

  ]
}